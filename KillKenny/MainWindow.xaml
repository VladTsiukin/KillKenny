<Window x:Class="KillKenny.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KillKenny"
        mc:Ignorable="d"
        ResizeMode="CanMinimize" WindowState="Maximized"
        x:Name="w" Title="Kenny Game Present">
    <Window.Triggers>
        <EventTrigger RoutedEvent="Window.Loaded">
            <EventTrigger.Actions>
                <BeginStoryboard>
                    <Storyboard x:Name="storyboardEllipse">
                        <DoubleAnimationUsingKeyFrames x:Name="dAnimation"
                                                       Storyboard.TargetName="ell" 
                                                       Storyboard.TargetProperty="(Canvas.Top)" >
                            <SplineDoubleKeyFrame x:Name="start" KeyTime="0:0:4"
                                                  Value="{Binding Path=Start, 
                                        RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}}" 
                                                  KeySpline="0.25,0 0.5,0.7"></SplineDoubleKeyFrame>
                            <SplineDoubleKeyFrame x:Name="end" KeyTime="0:0:8" Value="1000" 
                                                  KeySpline="0.25,0.8 0.2,0.4"></SplineDoubleKeyFrame>
                        </DoubleAnimationUsingKeyFrames>
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger.Actions>
        </EventTrigger>
    </Window.Triggers>
    <Window.InputBindings>
        <KeyBinding Key="{Binding SpeedUpCommand.GestureKey}" Command="{Binding SpeedUpCommand}"/>
        <KeyBinding Key="{Binding SpeedDownCommand.GestureKey}" Command="{Binding SpeedDownCommand}"/>
        <KeyBinding Key="{Binding FireCommand.GestureKey}" Command="{Binding FireCommand}"/>
        <KeyBinding Key="{Binding LeftRunCommand.GestureKey}" Command="{Binding LeftRunCommand}"/>
        <KeyBinding Key="{Binding RightRunCommand.GestureKey}" Command="{Binding RightRunCommand}"/>
        <KeyBinding Key="{Binding UpRunCommand.GestureKey}" Command="{Binding UpRunCommand}"/>
        <KeyBinding Key="{Binding DownRunCommand.GestureKey}" Command="{Binding DownRunCommand}"/>
    </Window.InputBindings>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.05*"></RowDefinition>
            <RowDefinition Height="0.05*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="1" Grid.Row="0" Background="#48424C">
            <ToolBar Background="#48424C" Foreground="White">
                <Button x:Name="Btnkeys" Foreground="White" Margin="5 0 10 3" Content="Keys"
                        Click="Btnkeys_Click"></Button>
                <Button x:Name="BtnProgram" Foreground="White" Margin="5 0 10 3" Content="Program" 
                        Click="BtnProgram_Click"></Button>
            </ToolBar>
        </StackPanel>
        <DockPanel Grid.Row="1">
            <DockPanel.Background>
                <SolidColorBrush Color="#48424C"></SolidColorBrush>
            </DockPanel.Background>
            <ProgressBar x:Name="pb" HorizontalAlignment="Left" Width="150" Value="12.0" Minimum="10.0" 
                         Maximum="30.0" Height="25"/>
            <!-- Labels info -->
            <Label Foreground="White" HorizontalAlignment="Left" x:Name="lb" VerticalAlignment="Center"
                   FontFamily="{StaticResource FA}">&#xf027;</Label>
            <CheckBox Command="{Binding SoundCommand}" VerticalAlignment="Center"
                      CommandParameter="{Binding IsChecked, RelativeSource={RelativeSource Self}}"></CheckBox>

            <DockPanel HorizontalAlignment="Right">
                <Button FontFamily="{StaticResource FA}" Background="Black" Width="30" Foreground="White"
                    VerticalAlignment="Center" Command="{Binding RestartCommand}">&#xf01e;</Button>
                <Label Foreground="White" Width="200" x:Name="lbShow" VerticalAlignment="Center" 
                   FontFamily="{StaticResource FA}" ></Label>
            </DockPanel>
        </DockPanel>
        <Canvas Grid.Row="2" x:Name="MainCanvas">
            <Canvas.Background>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1" >
                    <GradientStop Color="White" Offset="0" />
                    <GradientStop Color="DeepSkyBlue" Offset="1.0" />
                </LinearGradientBrush>
            </Canvas.Background>
            <Canvas.Triggers>
                <EventTrigger RoutedEvent="Canvas.Loaded">
                    <EventTrigger.Actions>
                        <BeginStoryboard>
                            <Storyboard x:Name="storyMainCanvas">
                                <ColorAnimation Storyboard.TargetName="MainCanvas"
                                        Storyboard.TargetProperty="Background.GradientStops[1].Color"
                                        To="SkyBlue" Duration="0:0:6" AutoReverse="True"
                                        RepeatBehavior="Forever"></ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger.Actions>
                </EventTrigger>
            </Canvas.Triggers>
            <!-- GUN AND CARTMAN -->
            <Ellipse Visibility="Hidden" x:Name="ellForFork" Canvas.Bottom="33" Panel.ZIndex="25" Canvas.Left="180"
                     Width="50" Height="50" Fill="Yellow"></Ellipse>
            <local:Cannon x:Name="gun" Canvas.Bottom="-10" Canvas.Left="160" Width="200" Height="150"></local:Cannon>
            <!-- KENNY -->
            <Rectangle x:Name="ell" Width="60" Height="40" 
                     Canvas.Right="{Binding Path=Start, 
                     RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}}"
                     Canvas.Top="{Binding Path=TopKenny, 
                     RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}}">
                <Rectangle.Fill>
                    <ImageBrush ImageSource="Images/Kenny.png"></ImageBrush>
                </Rectangle.Fill>
            </Rectangle>
            <!-- SUN -->
            <Ellipse Width="138" Height="141" Name="sun" Panel.ZIndex="-33" Canvas.Top="10" Canvas.Left="20" 
                     Stretch="Uniform" RenderTransformOrigin="0.54,0.492">
                <Ellipse.Fill>
                    <RadialGradientBrush
                         RadiusX="1" RadiusY="1" GradientOrigin="0.7,0.3">
                        <GradientStop Color="White" Offset="0" ></GradientStop>
                        <GradientStop Color="Gold" Offset="1" ></GradientStop>
                    </RadialGradientBrush>
                </Ellipse.Fill>
                <Ellipse.Triggers>
                    <EventTrigger RoutedEvent="Ellipse.Loaded">
                        <EventTrigger.Actions>
                            <BeginStoryboard>
                                <Storyboard x:Name="storySun">
                                    <PointAnimation Storyboard.TargetName="sun"
                                        Storyboard.TargetProperty="Fill.GradientOrigin"
                                        From="0.7,0.3" To="0.3,0.7" Duration="0:0:6"
                                        AutoReverse="True" RepeatBehavior="Forever"></PointAnimation>
                                    <ColorAnimation Storyboard.TargetName="sun"
                                        Storyboard.TargetProperty="Fill.GradientStops[1].Color"
                                        To="Goldenrod" Duration="0:0:6" AutoReverse="True"
                                        RepeatBehavior="Forever"></ColorAnimation>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                </Ellipse.Triggers>
            </Ellipse>
            <!-- HIT TO GOAL -->         
            <Label x:Name="label" Panel.ZIndex="30" FontFamily="{StaticResource FA}"
                   Foreground="Black" Opacity="0.1" FontSize="100"></Label>
            <Label x:Name="lbHit" Panel.ZIndex="30" Foreground="DarkGoldenrod" FontSize="30" Content="5 Points"
                   Canvas.Bottom="10" Canvas.Right="150"></Label>
            <!-- Back sound -->
            <MediaElement Source="Media\SLOT_they_kill_kenny.wav" Name="myMediaElement" 
                          Volume="0.05"
                          LoadedBehavior="Manual"/>
            <!-- Mounts -->
            <Grid  Canvas.Left="0" Canvas.Bottom="0" Width="{Binding ElementName=MainCanvas, 
                Path=ActualWidth}" Height="{Binding ElementName=MainCanvas, Path=ActualHeight}">
